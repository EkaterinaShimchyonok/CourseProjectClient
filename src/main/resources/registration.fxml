<?xml version="1.0" encoding="UTF-8"?>
<!--    "org.example.courseproject.ClientController"   -->

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:controller="org.example.courseproject.ClientController"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111"
          alignment="CENTER" hgap="10" vgap="10">
    <Label fx:id="responseLabel" text="" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2"/>

    <Label text="Email:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <TextField fx:id="emailInput" GridPane.columnIndex="1" GridPane.rowIndex="1"/>


    <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <PasswordField fx:id="passwordInput" GridPane.columnIndex="1" GridPane.rowIndex="2"/>

    <Button text="Register" onAction="#handleRegisterAction" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
</GridPane>